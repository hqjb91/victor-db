{
  "name": "victor-db-ts",
  "version": "1.0.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "test": "tsc -p tsconfig.test.json && node build-tests/tests/victordb.test.js",
    "format": "prettier . --write"
  },
  "keywords": [
    "Vector Database",
    "Vector Search",
    "Embeddings",
    "Local First",
    "Retrieval Augmented Generation"
  ],
  "author": "He Quanjie Boey",
  "license": "ISC",
  "description": "A local-first vector database for efficient storage and retrieval of embeddings.",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "prettier": "^3.7.2",
    "rollup": "^2.79.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@huggingface/transformers": "^3.8.0",
    "idb": "^8.0.3"
  },
  "peerDependencies": {
    "@huggingface/transformers": "^3.8.0",
    "idb": "^8.0.3"
  }
}
